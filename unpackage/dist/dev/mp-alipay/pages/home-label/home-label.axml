<view class="label"><view class="label-box"><view class="label-header"><view class="label-title">我的标签</view><view data-event-opts="{{[['tap',[['editLabel',['$event']]]]]}}" class="label-edit" onTap="__e">{{!is_edit?'编辑':'完成'}}</view></view><view class="label-content"><block a:if="{{loading}}"><uni-load-more vue-id="617f9216-1" status="loading" iconType="snow" onVueInit="__l"></uni-load-more></block><block a:for="{{labelList}}" a:for-item="item" a:for-index="index" a:key="_id"><block a:if="{{!loading}}"><view class="label-content__item">{{''+item.name+''}}<block a:if="{{is_edit}}"><uni-icons class="icons-close" vue-id="{{'617f9216-2-'+index}}" type="clear" size="20" color="red" data-event-opts="{{[['^click',[['del',[index]]]]]}}" onClick="__e" onVueInit="__l"></uni-icons></block></view></block></block><block a:if="{{labelList.length===0&&!loading}}"><view class="no-data">没有数据</view></block></view></view><view class="label-box"><view class="label-header"><view class="label-title">标签推荐</view></view><view class="label-content"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="_id"><block a:if="{{!loading}}"><view data-event-opts="{{[['tap',[['add',[index]]]]]}}" class="label-content__item" onTap="__e">{{''+item.name+''}}</view></block></block><block a:if="{{list.length===0&&!loading}}"><view class="no-data">没有数据</view></block></view></view></view>